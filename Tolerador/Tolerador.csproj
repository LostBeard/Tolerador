<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Version>2.2.77-alpha1</Version>
		<CompressionEnabled>false</CompressionEnabled>

		<SpawnDevBrowserExtensionPlatforms>Chrome;Firefox</SpawnDevBrowserExtensionPlatforms>
		
		<WebWorkerPatchFramework>true</WebWorkerPatchFramework>
		
		<!-- 
		<_DebugSpawnDevBrowserExtensionBuildTasks>true</_DebugSpawnDevBrowserExtensionBuildTasks>
		<_SpawnDevBrowserExtensionBuildTasksAssembly>D:\users\tj\Projects\SpawnDev.BlazorJS.BrowserExtension\SpawnDev.BlazorJS.BrowserExtension\SpawnDev.BlazorJS.BrowserExtension\tasks\netstandard2.0\SpawnDev.BlazorJS.BrowserExtension.Build.dll</_SpawnDevBrowserExtensionBuildTasksAssembly>
		-->
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.3" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="8.0.3" PrivateAssets="all" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SpawnDev.BlazorJS.WebWorkers" Version="2.2.82" />
		<PackageReference Include="SpawnDev.BlazorJS.BrowserExtension" Version="2.2.82-alpha2" />
	</ItemGroup>

</Project>
